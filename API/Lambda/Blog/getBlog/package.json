{
  "name": "GetBlog",
  "version": "1.0.0",
  "description": "Lambda permettant de récupérer les articles du blog de Monkenewha",
  "main": "index.js",
  "scripts": {
    "build": "tsc &&  7z a -tzip ../../..//getBlog.zip .// -xr!node_modules//aws-sdk -xr!node_modules//@types",
    "clean": "rmdir /Q /S dist && del ../../..//getBlog.zip",
    "local-run": "ts-node index.ts",
    "typecheck": "tsc --noEmit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1104.0",
    "moment": "^2.29.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.5"
  }
}