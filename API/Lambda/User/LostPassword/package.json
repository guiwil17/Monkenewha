{
  "name": "lostPassword",
  "version": "1.0.0",
  "description": "Lambda permettant d'ativer la procédure de mot de passe oublié sur Monkenewha",
  "main": "index.js",
  "scripts": {
    "build": "tsc &&  7z a -tzip ../../..//lostPassword.zip .// -xr!node_modules//aws-sdk -xr!node_modules//@types",
    "clean": "rmdir /Q /S dist && del ../../..//lostPassword.zip",
    "local-run": "ts-node index.ts",
    "typecheck": "tsc --noEmit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1104.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.5",
    "@types/nodemailer": "^6.4.4"
  }
}
